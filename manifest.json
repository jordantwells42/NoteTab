{   
    "name": "NoteTab",
    "manifest_version": 3,  
    "version": "0.0.0.2",
    "description": "A New Tab with notes!",
    "background": {
      "service_worker": "background.js"
    },

    "icons": {
    "128": "icon.png",
    "64": "icon.png",
    "48": "icon.png",
    "32": "icon.png",
    "16": "icon.png"
  },
  "chrome_url_overrides":{
    "newtab":"notetab.html"
  },

  "permissions": ["tabs"],
    "action": {
      "default_popup": "popup.html"
    }
}